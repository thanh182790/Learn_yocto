+#define LCD_WIDTH 		84
+#define LCD_HEIGHT 		48
+
+#define LCD_CMD			0
+#define LCD_DATA		1
+
+struct spi_device *nokia5110_spi;
+
+static int re_pin = 25;
+static int dc_pin = 24;
+//static int ce_pin = 26;
+
